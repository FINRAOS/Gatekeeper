{
  "name": "gatekeeper-ui",
  "version": "1.0.0",
  "description": "The UI for Gatekeeper",
  "main": "app/index.js",
  "scripts": {
    "prestart": "npm install",
    "start": "export NODE_ENV=dev && node node_modules/webpack-dev-server/bin/webpack-dev-server.js --hot --color --content-base app/ --port 9000",
    "serve": "export NODE_ENV=dev && node node_modules/.bin/webpack serve --config webpack.config.js --hot --color --progress --content-base app/ --port 9000",
    "win-start": "set NODE_ENV=dev && node node_modules/webpack-dev-server/bin/webpack-dev-server.js --hot --color --content-base app/ --port 9000",
    "build": "export NODE_ENV=production && node node_modules/.bin/webpack && cp -r app/assets dist",
    "win-build": "set NODE_ENV=production && node_modules\\.bin\\webpack && mkdir dist\\assets && copy app\\assets dist\\assets",
    "pretest": "npm install",
    "test": "export NODE_ENV=dev && karma start",
    "win-test": "set NODE_ENV=dev && karma start",
    "ci-test": "export NODE_ENV=dev && karma start --single-run=true --browsers=ChromeHeadless",
    "coverage": "export NODE_ENV=dev && karma start karma.conf.js --coverage",
    "ci-coverage": "export NODE_ENV=dev && karma start --single-run=true --browsers=ChromeHeadless --coverage",
    "test-single-run": "export NODE_ENV=dev && karma start karma.conf.js  --single-run",
    "preupdate-webdriver": "npm install",
    "update-webdriver": "webdriver-manager update",
    "preprotractor": "npm run update-webdriver",
    "protractor": "protractor e2e-tests/protractor.conf.js",
    "update-index-async": "node -e \"require('shelljs/global'); sed('-i', /\\/\\/@@NG_LOADER_START@@[\\s\\S]*\\/\\/@@NG_LOADER_END@@/, '//@@NG_LOADER_START@@\\n' + sed(/sourceMappingURL=angular-loader.min.js.map/,'sourceMappingURL=bower_components/angular-loader/angular-loader.min.js.map','app/bower_components/angular-loader/angular-loader.min.js') + '\\n//@@NG_LOADER_END@@', 'app/index-async.html');\"",
    "p": "node node_modules/.bin/webpack"
  },
  "devDependencies": {
    "@babel/core": "^7.14.0",
    "@babel/polyfill": "^7.12.1",
    "@babel/preset-env": "^7.14.1",
    "babel-loader": "^8.2.2",
    "babel-polyfill": "^6.26.0",
    "core-util-is": "^1.0.2",
    "css-loader": "^5.2.4",
    "del": "^1.2.0",
    "file-loader": "^0.8.4",
    "glob": "^5.0.15",
    "html-loader": "^2.1.2",
    "html-webpack-plugin": "^5.3.1",
    "imports-loader": "^0.6.5",
    "isparta-loader": "^2.0.0",
    "istanbul-instrumenter-loader": "^3.0.1",
    "jasmine-core": "^2.3.4",
    "jquery": "^2.1.4",
    "karma": "^6.3.2",
    "karma-babel-preprocessor": "^7.0.0",
    "karma-chrome-launcher": "^3.1.0",
    "karma-coverage": "^2.0.3",
    "karma-htmlfile-reporter": "^0.3.8",
    "karma-jasmine": "^4.0.1",
    "karma-junit-reporter": "^2.0.1",
    "karma-ng-html2js-preprocessor": "^1.0.0",
    "karma-sourcemap-loader": "^0.3.8",
    "karma-spec-reporter": "0.0.32",
    "karma-webpack": "^5.0.0",
    "less": "^2.5.3",
    "less-loader": "^2.2.0",
    "raw-loader": "^0.5.1",
    "style-loader": "^2.0.0",
    "svgo-loader": "^3.0.0",
    "terser-webpack-plugin": "^5.1.1",
    "url-loader": "^0.5.6",
    "util-deprecate": "^1.0.2",
    "webpack": "^5.37.0",
    "webpack-cli": "^4.7.0",
    "webpack-core": "^0.6.9",
    "webpack-dev-server": "^3.11.2"
  },
  "files": [
    "app/**/!(app).js",
    "app/**/*.tpl.html",
    "app/**/*.css",
    "package.json",
    "README.md"
  ],
  "dependencies": {
    "angular": "1.5.5",
    "angular-animate": "1.5.5",
    "angular-aria": "1.5.5",
    "angular-material": "1.1.1",
    "angular-material-data-table": "0.10.9",
    "angular-messages": "1.5.5",
    "angular-mocks": "1.5.5",
    "angular-resource": "1.5.5",
    "angular-ui-router": "0.2.18",
    "file-saver": "^2.0.2",
    "hover.css": "^2.0.2",
    "json2csv": "^4.5.4",
    "material-design-icons": "2.2.3",
    "moment": "2.10.6"
  }
}
